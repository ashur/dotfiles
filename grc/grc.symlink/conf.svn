### SVN
# conflict
regexp=^C .*$
colours=bold red
count=more
======
# modified
regexp=^M .*$
colours=blue
count=more
======
# added
regexp=^A .*$
colours=cyan
count=more
======
# removed
regexp=^D .*$
colours=red
count=more
======
# unlisted
regexp=^\? .*$
colours=bold yellow
count=more
======
# updated
regexp=^U .*$
colours=green
count=more
======
# fetching
regexp=^Fetching.*
colours=bold white
count=more
